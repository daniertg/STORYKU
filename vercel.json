{
    "version": 2,
    "builds": [
      { "src": "frontend/src/index.js", "use": "@vercel/static-build" },
      { "src": "backend/index.js", "use": "@vercel/node" }
    ],
    "routes": [
      { "src": "/api/(.*)", "dest": "/backend/index.js" },
      { "src": "/(.*)", "dest": "/frontend/src/$1" }
    ]
  }
  